{
  "name": "my-gulp-starter",
  "version": "1.0.0",
  "description": "A bare-bones gulp development setup for starting projects. ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Matthew Orndoff",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.2",
    "browserify": "^13.1.1",
    "eslint-config-google": "^0.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^3.0.1",
    "gulp-sass": "^2.3.2",
    "tap-spec": "^4.1.1",
    "tap-summary": "^3.0.1",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always"
      ],
      "no-cond-assign": [
        "error",
        "always"
      ],
      "no-console": "off"
    }
  }
}
